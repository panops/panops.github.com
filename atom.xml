---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>PanOps Platform</title>
  <link href="http://www.panops.org/atom.xml" rel="self" />
  <link href="http://www.panops.org/" />
  <updated>{{  site.time | date_to_xmlschema }}</updated>
  <id>http://www.panops.org</id>
  <author>
    <name>Panops Platform contributors</name>
    <email>siteadmin@panops.org</email>
  </author>
  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://www.panops.org{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://www.panops.org{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }} </content>
  </entry>
  {% endfor %}
</feed>
